@model ProjectCinderella.Model.UI.AlbumSpeedModel

<script>
    c3.generate({
        bindto: '#@Model.ID',
        data: {
            columns: [
                ['33 1/3 rpm', @Model.Num33RPM],
                ['45 rpm', @Model.Num45RPM],
                ['78 rpm', @Model.Num78RPM]
            ],
            type: 'donut'
        },
        donut: {
            title: "@Model.Title",
            label: {
                format: function (value) { return value; }
            }
        }
    });
</script>