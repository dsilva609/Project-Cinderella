@model UI.Models.GameRatingModel

	<script>
		c3.generate({
			bindto: '#@Model.ID',
			data: {
				columns: [
					['EC', @Model.NumRatedEC],
					['E', @Model.NumRatedE],
					['E-10', @Model.NumRatedE10],
					['T', @Model.NumRatedT],
					['M', @Model.NumRatedM],
					['A', @Model.NumRatedA]
				],
				type: 'donut'
			},
			donut: {
				title: "@Model.Title",
				label: {
					format: function (value) { return value; }
				}
			}
		});
	</script>