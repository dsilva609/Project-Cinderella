@model UI.Models.PopStatsViewModel

<div class="collapse in">
	<div class="panel-body">
		<ul class="list-group col-md-6">
			<li class="list-group-item active">
				Top Series
			</li>
			@foreach (var series in Model.TopSeries)
			{
				<li class="list-group-item">
					@series
				</li>
			}
		</ul>
	</div>
</div>

