@model UI.Models.GameStatsViewModel

<div class="collapse in">
	<div class="panel-body">
		<ul class="list-group col-md-6">
			<li class="list-group-item active">
				Game Type Stats
			</li>
			<li class="list-group-item">
				Full Game Count
				<span class="tag tag-info tag-pill pull-xs-right">@Model.NumFullGame</span>
			</li>
			<li class="list-group-item">
				DLC Count
				<span class="tag tag-info tag-pill pull-xs-right">@Model.NumDLC</span>
			</li>
			<li class="list-group-item">
				Expansion Count
				<span class="tag tag-info tag-pill pull-xs-right">@Model.NumExpansion</span>
			</li>
		</ul>
	</div>
</div>

